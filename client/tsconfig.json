{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": [
      "react",
      "react-native",
      "expo"
    ],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "importHelpers": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "extends": "expo/tsconfig.base"
}
